<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="PracticaDSMGen.Infraestructure"
		                   namespace="PracticaDSMGen.Infraestructure.EN.PracticaDSM">
		    
		    
			
				<class name="DesafioNH" table="Desafio"> 
				  	
						<id name="Id" column="idDesafio" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Nombre" type="string"> 
						<column name="nombre"  not-null="true"/>
					</property>
					<property name="Descripcion" type="string"> 
						<column name="descripcion"  not-null="true"/>
					</property>
			
			
			
			
			
		  			
						<bag name="Insignia" table="[desafio_insignia]"  >
							<key>
								
									<column name="FK_idDesafio_idDesafio"/> 
							</key>
      						<many-to-many class="PracticaDSMGen.Infraestructure.EN.PracticaDSM.InsigniaNH">
									<column name="FK_idInsignia_idInsignia"/> 
      						</many-to-many>
    					</bag>
			
		  			
						<bag name="Videojuego" table="[videojuego_desafio]" inverse="true" >
							<key>
								
									<column name="FK_idDesafio_idDesafio"/> 
							</key>
      						<many-to-many class="PracticaDSMGen.Infraestructure.EN.PracticaDSM.VideojuegoNH">
									<column name="FK_idVideojuego_idVideojuego"/> 
      						</many-to-many>
    					</bag>
					
					
					
					
					
		
		 </class>	
		  
		  <query name="DesafioNHReadAllHQL">from DesafioNH</query>
		  <query name="DesafioNHobtenerDesafioPorVideojuegoHQL"><![CDATA[select de FROM DesafioNH as de join  de.Videojuegos as v where v.id = :p_idVideojuego]]></query>
		</hibernate-mapping>
	